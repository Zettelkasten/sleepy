func Hello() ->  Str  {
  @Mutable str = EmptyStr();
  insert(str, 'H');
  insert(str, 'e');
  insert(str, 'l');
  insert(str, 'l');
  insert(str, 'o');
  assert(str.length == 5);
  return str;
}

func main()  {
  hello = Hello();
  world = "World";
  print_line(hello + ", " + world + '!');
  free(hello);  # actually we need to free all intermediate values too, but blargh!
}
