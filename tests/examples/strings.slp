func Hello() -> Str {
  @Mutable str = EmptyStr();
  insert(str, 'H');
  insert(str, 'e');
  insert(str, 'l');
  insert(str, 'l');
  insert(str, 'o');
  assert(str.length == 5);
  return str;
}

func main() {
  hello = Hello();
  print_line(hello);
  print_line(hello + ',' + ' ' + hello + '!');
  free(hello);
}
