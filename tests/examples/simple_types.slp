func main() {
  Int my_int = 21;
  Double my_double = 4.0;
  Double prod = my_double * my_double;
  print(prod);
  print('\n');
  sum = my_double + my_double;  # also implicitly a Double
  print(sum);
  print('\n');
  int_calc = my_int * 34;

  @RefType
  struct IntPair {
    Int left = 0;
    Int right = 0;
  }

  # @Const can also be omitted.
  func get_left(@Const IntPair pair) -> Int {
    return pair.left;
  }
  func set_left(@Mutable IntPair pair, Int new_left) {
    pair.left = new_left;
  }
}
